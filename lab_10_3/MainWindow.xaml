<Window x:Class="lab_10_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_10_3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ToolBar Grid.Row="0" Grid.ColumnSpan="2">
            <Button Content="Сгенерировать"/>
            <Separator/>
            <Button Content="Выполнить"/>
        </ToolBar>
        <TextBlock x:Name="Table1" Grid.Column="0" Grid.Row="1"
                   Margin="10"/>
        <TextBlock x:Name="Table2" Grid.Column="1" Grid.Row="1"
                   Margin="10"/>
    </Grid>
</Window>
